<?xml version="1.0"?>
<page layout="1column" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
  <body>
    <referenceBlock name="product.info.details">                
      <block class="AJH\ProductVehicle\Block\Product\View\FitmentList" name="product-vehicle" template="AJH_ProductVehicle::catalog/product/view/fitmentlist.phtml" group="detailed_info">
        <arguments>
          <argument name="at_call" xsi:type="string">getList</argument>
          <argument name="at_code" xsi:type="string">productlist</argument>
          <argument name="css_class" xsi:type="string">productlist</argument>
          <argument name="at_label" xsi:type="string">productlist</argument>
          <argument name="add_attribute" xsi:type="string">itemprop="productlist"</argument>
          <argument name="title" translate="true" xsi:type="string">Fitment Vehicle</argument>
          <argument name="priority" xsi:type="string">1</argument>
        </arguments>
      </block>
    </referenceBlock>
  </body>
</page>